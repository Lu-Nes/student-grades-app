{
  "compilerOptions": {
    "target": "ES6",
    "module": "ES2022",
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noEmit": true,
    "moduleResolution": "node",
    "types": ["node", "express", "cors"]
  },
  "include": ["src"],
  "exclude": ["node_modules"],
}
